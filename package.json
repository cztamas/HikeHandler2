  "description": "very hikehandler",
  "main": "index.js",
  "scripts": {
    "backup": "node ./scripts/old-db-backup/backup.js",
    "heroku-postbuild": "webpack --config webpack.prod.config.js --mode=production",
    "docker": "docker-compose up -d",
    "start-client": "webpack-dev-server --config webpack.dev.config.js",
