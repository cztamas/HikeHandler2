{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "warn",
            "tab"
        ],
        "no-console": "off",
        "no-unused-vars": "warn",
        "quotes": [
            "warn",
            "single"
        ],
        "react/prop-types": "off",
        "semi": [
            "warn",
            "always"
        ]
    },
    "globals": {
        "gapi": false
    }
}